    "# Create a class and a function that checks whether the given number is Odd or Even \n",
    "# OddEven.OddEven() \n",
    "# Enter a number: 52452 \n",
    "# 52452 is Even number \n",
    "\n",
    "class OddEven():\n",
    "    def OddEven():\n",
    "        num = int(input(\"Enter a number:\"))\n",
    "        if((num%2) == 0):\n",
    "            print(num,\"is Even Number\")\n",
    "        else:\n",
    "            print(num,\"is Odd Number\")"
   ]
